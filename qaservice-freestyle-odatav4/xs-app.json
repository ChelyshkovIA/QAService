{
    "authenticationMethod": "route",
    "routes": [
        {
            "source": "^/app/(.*)$",
            "target": "/$1",
            "localDir": "./"
        },
        {
            "source": "^/odatav2/(.*)$",
            "target": "/$1",
            "destination": "qaservice-odatav2",
        	"authenticationType": "xsuaa",
            "csrfProtection": true
        },
        {
            "source": "^/odatav4/(.*)$",
            "target": "/$1",
            "destination": "qaservice-odatav4",
        	"authenticationType": "xsuaa",
            "csrfProtection": true
        }
    ]
}